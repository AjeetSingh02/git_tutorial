git --version
touch index.html
git init
git config --global user.name 'Ajeet Singh'
git add index.html
git status
git rm --cached index.html
git add *.html
git add .
git commit
git commit -m 'changed app.js'
touch .gitignore      	
	to exclude some miles from being committed

git branch login
git checkout login
git merge login

git remote add origin https://github.com/AjeetSingh02/git_tutorial.git


git remote
git remote rm origin

git push -u origin master


